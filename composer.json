{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "aws/aws-sdk-php": "^3.314.0",
        "ladumor/laravel-pwa": "^0.0.4",
        "laravel/framework": "v11.11.1",
        "laravel/tinker": "v2.9.0",
        "yajra/laravel-datatables-oracle": "v11.1.1"
    },
    "require-dev": {
        "fakerphp/faker": "v1.23.1",
        "laravel/pint": "v1.16.1",
        "laravel/sail": "v1.29.3",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "v8.1.1",
        "phpunit/phpunit": "^11.2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@/usr/local/bin/php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@/usr/local/bin/php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@/usr/local/bin/php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@/usr/local/bin/php artisan key:generate --ansi",
            "@/usr/local/bin/php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@/usr/local/bin/php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
