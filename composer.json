{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "v5.0.0",
        "illuminate/html": "v5.0.0",
		"illuminate/support": "v5.8.36",
        "doctrine/dbal": "^2.13.9",
		"yajra/laravel-datatables-oracle": "v6.29.3",
        "mgallegos/laravel-jqgrid": "v1.3.0",
        "anhskohbo/no-captcha": "2.4.0"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist",
		"allow-plugins": {
			"kylekatarnls/update-helper": true
		}
	}
}
